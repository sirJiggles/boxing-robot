    "start": "yarn build && yarn execute",
    "execute": "node dist/index.js",
    "lint": "eslint '**/*.{js,ts}' --fix"
  },
  "dependencies": {
    "johnny-five": "^2.0.0",
    "raspi-io": "^11.0.0"
  },
  "devDependencies": {
    "@boxingrobot/eslint-config": "1.0.0",
    "@types/johnny-five": "1.3.0",
    "@types/node": "^14.14.6",
    "@types/node-fetch": "2.5.8",
    "eslint": "^7.12.1",
    "typescript": "^4.0.5"
  }
}
