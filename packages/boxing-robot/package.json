{
  "name": "@boxingrobot/boxing-robot",
  "version": "1.0.0",
  "description": "The package containing the boxing robot",
  "main": "dist/index.js",
  "author": "Gareth Fuller",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "package": "rm -rf dist && yarn build && cp package.json dist/ && cd dist && npm install --production",
